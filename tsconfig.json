{
  "compilerOptions": {
    "strict": false,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    //配置
    "baseUrl": "./",
    "paths": {
      "@/*": ["src/*"],
      "@": ["src"]
    },
    "allowImportingTsExtensions": true,
    "noEmit": true
  },
  "include": ["**/*.d.ts", "src/**/*.ts", "src/**/*.tsx", "src/**/*.vue", "tests/**/*.ts", "tests/**/*.tsx"]
}
